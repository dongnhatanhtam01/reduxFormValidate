      <div className=\"form-group col-6\">\r\n                  <span>Mã Sinh Viên</span>\r\n                  {/* form-student-code  - validated 0 -> 36*/}\r\n                  <input\r\n                    pattern=\"^([1-9]|[12]\\d|3[0-6])$\"\r\n                    className=\"form-control\"\r\n                    name=\"maSV\"\r\n                    value={this.state.values.maSV}\r\n                    placeholder={this.state.values.maSV}\r\n                    onChange={this.onChangeHandler}\r\n                  />\r\n                  <p className=\"text-danger\">\r\n                    {this.state.errors.maSV}sss\r\n                  </p>\r\n                </div>\r\n                <div className=\"form-group col-6\">\r\n                  <span>Họ và tên</span>\r\n                  {/* form-student-name  */}\r\n                  <input\r\n                    className=\"form-control\"\r\n                    name=\"hoTen\"\r\n                    value={this.state.values.hoTen}\r\n                    placeholder={this.state.values.hoTen}\r\n                    onChange={this.onChangeHandler}\r\n                  />\r\n                  <p className=\"text-danger\">\r\n                    {this.state.errors.hoTen}\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div className=\"row\">\r\n                <div className=\"form-group col-6\">\r\n                  <span>Số điện thoại</span>\r\n                  {/* form-student-contact  */}\r\n                  <input\r\n                    type=\"text\"\r\n                    pattern=\"^(0|[1-9][0-9]*)$\"\r\n                    className=\"form-control\"\r\n                    name=\"soDienThoai\"\r\n                    value={this.state.values.soDienThoai}\r\n                    placeholder={this.state.values.soDienThoai}\r\n                    onChange={this.onChangeHandler}\r\n                  />\r\n                  <p className=\"text-danger\">\r\n                    {this.state.errors.soDienThoai}\r\n                  </p>\r\n                </div>\r\n                <div className=\"form-group col-6\">\r\n                  <span>Email</span>\r\n                  {/* form-student-emailAddress  */}\r\n                  <input\r\n                    type=\"text\"\r\n                    pattern=\"^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$\"\r\n                    className=\"form-control\"\r\n                    name=\"email\"\r\n                    value={this.state.values.email}\r\n                    placeholder={this.state.values.email}\r\n                    onChange={this.onChangeHandler}\r\n                  />\r\n                  <p className=\"text-danger\">\r\n                    {this.state.errors.email}\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              <div className=\"row\">\r\n                <div className=\"col-md-12 text-end mt-4\">\r\n                  {this.state.valid ? <button className=\" btn btn-success\"  >Thêm đối tượng</button> : <button className=\" btn btn-success\" disabled >Thêm đối tượng</button>}\r\n                </div>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    mangSinhVien: state.QuanLySinhVienReducer.mangSinhVien || {}\r\n  }\r\n}\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    themSinhVien: (sinhVien) => {\r\n      const action = {\r\n        type: \"THEM_SINH_VIEN\",\r\n        sinhVien,\r\n      };\r\n      dispatch(action);\r\n    },\r\n  };\r\n};\r\nexport default connect(mapStateToProps, mapDispatchToProps)(FormSinhVien);\r\n","C:\\react_lms\\reduxForm\\validate-form-app\\src\\components\\BaiTapForm\\ModalSinhVien.js",[],{"ruleId":"48","severity":1,"message":"49","line":38,"column":8,"nodeType":"50","messageId":"51","endLine":38,"endColumn":14},{"ruleId":"48"